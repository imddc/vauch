{
  "name": "vauch-ts-starter",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "commitlint": "commitlint --edit",
    "lint": "oxlint ./src && eslint ./src",
    "lint:fix": "oxlint ./src --fix && eslint ./src --fix",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.27.3",
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@vitest/ui": "^2.1.4",
    "eslint": "^9.14.0",
    "eslint-plugin-oxlint": "^0.11.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.10",
    "oxlint": "^0.11.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vitest": "^2.1.4"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "oxlint ./src && eslint ./src"
    ]
  }
}
